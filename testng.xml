<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="FREE CRM Application Test Automation Test Suite">


    <test name="Experian Automation Smoke Test ">
        <parameter name = "browser" value="chrome"/>
        <groups>
            <run>
                <include name="SmokeTest"></include>
            </run>

        </groups>
        <packages>

            <package name="com.experian.qa.testcases"/>
        </packages>


    </test>

    <test name="Experian Automation Functional">
        <parameter name = "browser" value="chrome"/>
        <groups>
            <run>
                <include name="FunctionalTest"></include>
            </run>

        </groups>
        <packages>
            <package name="com.experian.qa.testcases"/>
        </packages>
    </test>

</suite>